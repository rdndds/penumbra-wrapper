{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "PenumbraWrapper",
  "version": "0.1.0",
  "identifier": "penumbra-wrapper",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "windows": [
      {
        "title": "PenumbraWrapper",
        "maximized": true,
        "minWidth": 800,
        "minHeight": 600,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  }
}
